{
  "name": "react-with-webpack",
  "version": "1.0.0",
  "description": "practice ReactJS setup configuration using Webpack",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d devtools && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs",
    "webpack"
  ],
  "author": "learndave",
  "license": "MIT",
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "babel-loader": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
